{
  "name": "boomtown-client",
  "version": "1.0.0",
  "description": "Client-side code for RED Academy's Boomtown project.",
  "scripts": {
    "build": "react-scripts build && rm -rf ../server/public && mv build ../server/public",
    "precommit": "npm run lint --quiet",
    "eject": "react-scripts eject",
    "lint": "npm run prettier -- --list-different",
    "lint:fix": "npm run prettier -- --write",
    "prettier": "prettier '**/*.js'",
    "start": "react-scripts start",
    "test": "npm run lint && react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.2",
    "@material-ui/icons": "^1.1.0",
    "apollo-cache-inmemory": "^1.2.4",
    "apollo-client": "^2.3.4",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "final-form": "^4.8.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.5",
    "react-dom": "^16.4.1",
    "react-final-form": "^3.6.2",
    "react-gravatar": "^2.6.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5"
  },
  "devDependencies": {
    "prettier": "1.13.7"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
